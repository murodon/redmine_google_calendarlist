<h2><%=h l(:label_gcal) %>#<%= @gcal.id %></h2>
<%= labelled_form_for :gcal, @gcal,
                  :url => project_gcal_path(@project),
                  :html => {:multipart => true, :id => 'gcal-form'} do |f| %>
    <%= render :partial => 'gcals/form', :locals => {:form => f} %>
    <%= f.submit l(:button_edit) %>
<% end %>

